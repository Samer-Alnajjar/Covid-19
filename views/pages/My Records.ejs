<!DOCTYPE html>
<html lang="en">
  <%- include('../layout/head.ejs') %>
  <body>
    <%- include('../layout/header.ejs') %>

    <div class="main">
      <%data.forEach( day => { %>
        <div class="card">
          <h3>Country: <%= day.country%></h3>
          <h3>Date: <%= day.date%></h3>
          <a href="/RecordDetails/<%= day.id%>">View Details</a>
        </div>
      <%})%>
    </div>
    

    <%- include('../layout/footer.ejs') %>
  </body>
</html>
